<div class="jumbotron">
  <h2><%= @store.store_name %></h2>
  <a class="btn btn-default btn" href="/stores/<%= @store.id %>/brands/new" role="button">Add a Shoe Brand</a>
</div>

<div class="container-fluid">
  <h4>Available Brands:</h4>

  <ul class="list-unstyled">
    <% @store.brands.each do |brand| %>
    <li><%= brand.brand_name %></li>
    <% end %>
  </ul>
</div>
